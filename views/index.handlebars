
<!-- Intro -->
<div id="index-banner" class="parallax-container main-title">
  <div class="section">
    <div class="container">
      <div class="row center">
        <div class="col s12" id="main-title">
          <h5 class="center" id="subtitle">Find and save your favorite articles from goop.com here</h5>
          <br>
        </div>
      </div>
    </div>
  </div>
 
<!-- Container -->
<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12">
        <br>
        <div class="row">
          <!-- Articles -->
          {{#each data}}
           <div class="col s12 m12">
           <ul class="collection">
    <li class="collection-item avatar">
      <img src="{{this.photo}}" alt="article-photo" class="circle">
      <a class="url" href="{{this.link}}"><span class="title">{{this.title}}</span> </a>
      <p class="summary">{{this.summary}}</p> <br>
         <a class="waves-effect waves-light btn secondary-content save-btn" data-id="{{this._id}}"><i class="material-icons right" >beenhere</i>Save Article</a>
    </li>
    </ul>
    </div>
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</div>